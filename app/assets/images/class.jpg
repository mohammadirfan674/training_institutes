            }
            if ((charCode > 64 && charCode < 91) || (charCode > 96 && charCode < 123) || (charCode == 8) || (charCode == 32) || (charCode == 9))
                return true;
            else
                return false;
        } catch (err) {
            alert(err.Description);
        }
    }
</script>
